  
  <div>Connect with 

    <%- if devise_mapping.omniauthable? %>
      <%- resource_class.omniauth_providers.each do |provider| %>
        <div class="share <%= provider.to_s %>">
          <%= link_to "<i class='fa fa-#{provider.to_s}'></i><span>#{provider.to_s.titleize}</span>".html_safe, omniauth_authorize_path(resource_name, provider) %>
        </div>
      <% end -%>
    <% end -%>

  </div>

  <p class="back_liner">
    <i class="back_line"></i>
    <span><strong><%= t('or') %></strong></span>
  </p>
